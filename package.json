{
  "name": "eslint-plugin-zero-config",
  "version": "2.2.9",
  "description": "A complete ESLint plugin for javascript projects",
  "repository": "https://github.com/exelord/eslint-plugin-zero-config",
  "main": "lib/index.js",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "javascript"
  ],
  "scripts": {
    "release": "release-it",
    "test": "jest"
  },
  "release-it": {
    "git": {
      "tagName": "v${version}",
      "commitMessage": "v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    }
  },
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-decorator-position": "^2.2.28",
    "eslint-plugin-ember": "^10.5.1",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-qunit": "^6.1.1"
  },
  "devDependencies": {
    "eslint": "^7.29.0",
    "glob": "^7.1.7",
    "jest": "^27.0.5",
    "release-it": "^14.10.0"
  },
  "peerDependencies": {
    "eslint": "^7.29.0"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
